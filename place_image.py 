y1, y2 = y_offset, y_offset + s_img.shape[0]
x1, x2 = x_offset, x_offset + s_img.shape[1]

alpha_emoji = emoji[:, :, 3] / 255.0
alpha_img = 1.0 - alpha_emoji

for c in range(0, 3):
    l_img[y1:y2, x1:x2, c] = (alpha_emoji * emoji[:, :, c] +
                              alpha_img * img[y1:y2, x1:x2, c])
